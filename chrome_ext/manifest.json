{
  "name": "Clipr",
  "version": "1.2",
  "description": "Boookmarks - but better.",
  "permissions": [
    "tabs", "activeTab", "http://localhost/*", "http://*/*", "https://*/*", "file://*/*", "bookmarks", "<all_urls>", "identity", "identity.email"
  ],
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["content.js","popup.js"]
  }],
  "oauth2": {
    "client_id": "313073444392-hcbme8h90e4m4vef5aqev4q2p0t4nugo.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/plus.login"]
  },
  "externally_connectable" : {
    "matches" :[
    "https://clipr-app-1.herokuapp.com/#/clips"]
    },

  "icons": {
    "16": "new_logo_ss.png",
    "42": "new_logo_ss.png",
    "128": "new_logo_ss.png"
  },
  "browser_action": {
    "default_title": "Clipr",
    "default_icon": "new_logo_ss.png",
    "default_popup": "popup.html"

  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "key": "",
  "manifest_version": 2

}